{
  "name": "vic-generator",
  "version": "0.0.6",
  "description": "A vitesse/nuxt/vue 3 cli for generating some stuff.",
  "author": "nadjem @nadjem",
  "bin": {
    "vic": "./bin/run"
  },
  "homepage": "https://github.com/nadjem/vic",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "nadjem/vic",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@inquirer/prompts": "^3.1.2",
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^3.8.0",
    "chmodr": "^1.2.0",
    "mock-fs": "^5.2.0",
    "nyc": "^15.1.0",
    "picocolors": "^1.0.0",
    "sinon-chai": "^3.7.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.5.5",
    "@types/chai": "^4",
    "@types/chmodr": "^1.0.1",
    "@types/mocha": "^9.0.0",
    "@types/mock-fs": "^4.13.2",
    "@types/node": "^16.18.53",
    "@types/sinon-chai": "^3.2.10",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "npm-watch": "^0.11.0",
    "oclif": "^3.17.1",
    "shx": "^0.3.4",
    "sinon": "^16.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "vic",
    "dirname": "vic",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "watch": {
    "build": {
      "patterns": [
        "src/**/*"
      ],
      "extensions": "ts",
      "quiet": false
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "watch": "npm-watch build",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "nyc mocha  \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/nadjem/vic/issues",
  "keywords": [
    "vue",
    "vitesse",
    "nuxt",
    "cli",
    "generator"
  ],
  "types": "dist/index.d.ts"
}
